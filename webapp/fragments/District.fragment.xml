<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">

		<SelectDialog  title="{i18n>districtFragTitle}" 
		liveChange="onLiveChangeDistrict"  multiSelect="true"
		confirm="onConfirmChangeDistrict" cancel="onCancelChangeSalesOrganization" items="{DistrictSet>/results}" class="sapUiSizeCompact">
		<StandardListItem
				title="{parts:[{path:'DistrictSet>Name'},{path:'DistrictSet>Division'}],formatter:'.formatter.f4ValueBind'}"
							tooltip="{parts:[{path:'DistrictSet>Name'},{path:'DistrictSet>Division'}],formatter:'.formatter.f4ValueBind'}" type="Active"></StandardListItem>
	</SelectDialog>
</core:FragmentDefinition>