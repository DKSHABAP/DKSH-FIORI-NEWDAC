<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">

		<SelectDialog  title="Country" 	liveChange="onLiveChangeCountry" 
		 multiSelect="true"
		confirm="onConfirmChangeCt" cancel="onCancelChangeSalesOrganization" items="{CtSet>/results}" class="sapUiSizeCompact">
		<StandardListItem
				title="{parts:[{path:'CtSet>countryName'},{path:'CtSet>countryCode'}],formatter:'.formatter.f4ValueBind'}"
							tooltip="{parts:[{path:'CtSet>countryName'},{path:'CtSet>countryCode'}],formatter:'.formatter.f4ValueBind'}" type="Active"></StandardListItem>
	</SelectDialog>
</core:FragmentDefinition>