<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
		<Dialog  title="{i18n>AssignedUserFragTitle}" contentHeight="40%" class="sapUiSizeCompact" ><content>
			<List inset="true" items="{displayUserModel>/}">
		<items>
			<StandardListItem title="{displayUserModel>userId}" info="{displayUserModel>email}" />
		</items>
	</List>
		
		</content>
			<buttons>
	
			<Button text="Close" press="onPressCloseFrg" ></Button>
			
		</buttons>
	</Dialog>
</core:FragmentDefinition>